e flex-container(\n        $direction: column,\n        $justify-content: space-around,\n        $align-items: center,\n        $wrap: nowrap\n    );\n    width: 150px;\n    height: 150px;\n    background-color: $white-color;\n    box-shadow: $primary-box-shadow;\n    border-radius: 20px;\n\n    &Text {\n        @include text($size: 1.25rem, $weight: 400, $color: $primary-color, $line-height: null);\n    }\n}\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n"},"lineCount":null}},"error":null,"hash":"184f3514555b79d63e4ecf824885dcd9","cacheData":{"env":{}}}