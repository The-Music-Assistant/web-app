e":null,"original":{"line":34,"column":17},"generated":{"line":1,"column":837}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":35,"column":4},"generated":{"line":1,"column":838}},{"source":"../../src/components/AuthCards/ProfileCard/ProfileCard.module.scss","name":null,"original":{"line":70,"column":40},"generated":{"line":1,"column":860}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":36,"column":4},"generated":{"line":1,"column":861}},{"source":"../../src/components/AuthCards/ProfileCard/ProfileCard.module.scss","name":null,"original":{"line":71,"column":47},"generated":{"line":1,"column":890}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":37,"column":4},"generated":{"line":1,"column":891}},{"source":"../../src/components/AuthCards/ProfileCard/ProfileCard.module.scss","name":null,"original":{"line":72,"column":43},"generated":{"line":1,"column":916}},{"source":"../../src/components/AuthCards/ProfileCard/ProfileCard.module.scss","name":null,"original":{"line":75,"column":5},"generated":{"line":1,"column":917}}],"sources":{"../../src/components/AuthCards/ProfileCard/ProfileCard.module.scss":"// ----------------------------------------------------------------------------\n// File Path: src/components/AuthCards/ProfileCard/ProfileCard.module.scss\n// Description: SCSS styles for the profile card component\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 1/6/2020\n// ----------------------------------------------------------------------------\n\n@import \"../../../root-styles/utilities.scss\";\n\n.profileCard {\n    &ImageInput {\n        @include flex-container(\n            $direction: column,\n            $justify-content: null,\n            $align-items: center,\n            $wrap: nowrap\n        );\n\n        &Img,\n        &ImgPlaceholder {\n            width: 70px;\n            height: 70px;\n            border: 2px solid $primary-color;\n            border-radius: 50%;\n\n            @include media-query($breakpoint: $tablet-breakpoint) {\n                width: 90px;\n                height: 90px;\n            }\n        }\n\n        &Img {\n            object-fit: cover;\n            margin-bottom: 6px;\n\n            &Container {\n                position: relative;\n            }\n        }\n\n        &ImgPlaceholder {\n            margin-bottom: 10px;\n            background-color: $orange-color;\n        }\n\n        &RemoveButton {\n            position: absolute;\n            top: 0;\n            right: -20px;\n            width: 15px;\n            height: 15px;\n\n            &Img {\n                width: 15px;\n                height: 15px;\n            }\n        }\n    }\n\n    &TextInput {\n        margin-top: 20px;\n    }\n\n    &TextInputs {\n        gap: 40px;\n\n        @include media-query($breakpoint: $tablet-breakpoint) {\n            @include grid-container(\n                $grid-template-rows: 1fr,\n                $grid-template-columns: 1fr 1fr,\n                $align-items: space-between\n            );\n        }\n    }\n}\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n"},"lineCount":null}},"error":null,"hash":"5eef44b0871e47172abf1021f4278104","cacheData":{"env":{}}}