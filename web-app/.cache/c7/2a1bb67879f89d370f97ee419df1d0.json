t - see comments in the JS file\n\n@import \"../../../root-styles/utilities.scss\";\n\n.selectInput {\n    position: relative;\n    z-index: 10000;\n\n    &InputElement {\n        display: none;\n    }\n\n    &Selector {\n        box-shadow: $primary-box-shadow;\n        transition: transform 0.2s ease-in-out;\n        @include flex-container(\n            $direction: row,\n            $justify-content: space-between,\n            $align-items: center,\n            $wrap: nowrap\n        );\n        height: 100%;\n        padding: 10px 20px;\n        border-radius: 10px;\n\n        &:hover {\n            transform: translateY(-2px);\n        }\n\n        &Title {\n            @include text($size: 1rem, $weight: 400, $color: null, $line-height: null);\n\n            @include media-query($breakpoint: $tablet-breakpoint) {\n                font-size: 1.25rem;\n            }\n        }\n\n        &ArrowImg {\n            margin-left: 15px;\n            // If width is changed to height, the ref in componentDidMount() will give an inaccurate width (not sure why)\n            width: 14px;\n            transition: transform 0.3s ease-in-out;\n\n            &Up {\n                transform: rotate(180deg);\n            }\n\n            &Down {\n                transform: rotate(0deg);\n            }\n\n            @include media-query($breakpoint: $tablet-breakpoint) {\n                margin-left: 30px;\n                width: 18px;\n            }\n        }\n    }\n\n    &Options {\n        position: absolute;\n        left: 0;\n        width: 100%;\n        border-radius: 10px;\n        box-shadow: $primary-box-shadow;\n        transition: transform 0.3s ease-in-out;\n\n        &Show {\n            transform: scaleY(1);\n            transform-origin: top;\n        }\n\n        &Hide {\n            transform: scaleY(0);\n            transform-origin: top;\n        }\n    }\n\n    &Option {\n        display: block;\n        @include text($size: 1rem, $weight: 300, $color: null, $line-height: null);\n\n        @include media-query($breakpoint: $tablet-breakpoint) {\n            font-size: 1.25rem;\n        }\n\n        text-align: center;\n        margin: 10px 0;\n        width: 100%;\n\n        &::after {\n            display: block;\n            content: \"\";\n            width: 100%;\n            height: 1px;\n            background-color: $white-color;\n            margin-top: 10px;\n        }\n\n        &:last-child {\n            &::after {\n                display: none;\n            }\n        }\n    }\n}\n\n.white {\n    background-color: $white-color;\n    color: $primary-color;\n}\n\n.primaryBlue {\n    background-color: $primary-color;\n    color: $white-color;\n}\n\n.green {\n    background-color: $green-color;\n    color: $white-color;\n}\n\n.orange {\n    background-color: $orange-color;\n    color: $white-color;\n}\n\n.red {\n    background-color: $red-color;\n    color: $white-color;\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}"},"lineCount":null}},"error":null,"hash":"7f5344dd3e439c16546cb57797981488","cacheData":{"env":{}}}