breakpoint) {\n        width: 250px;\n    height: 250px;\n    }\n\n    &Img {\n        object-fit: cover;\n        width: 70px;\n        height: 70px;\n        display: block;\n        margin: auto;\n        border-radius: 50%;\n\n        &Placeholder {\n            @include flex-container(\n                $direction: row,\n                $justify-content: center,\n                $align-items: center,\n                $wrap: nowrap\n            );\n            width: 70px;\n            height: 70px;\n            margin: auto;\n            border-radius: 50%;\n            background-color: $white-color;\n\n            &Letter {\n                @include text($size: 1.5rem, $weight: 400, $color: null, $line-height: null);\n            }\n        }\n    }\n\n    &Name {\n        @include text($size: 1.25rem, $weight: 400, $color: $white-color, $line-height: null);\n        text-align: center;\n        margin-top: 20px;\n    }\n\n    &Roles {\n        @include text($size: 0.875rem, $weight: 300, $color: $white-color, $line-height: null);\n        text-align: center;\n        margin-top: 20px;\n    }\n}\n\n.primaryBlue {\n    background-color: $primary-color;\n\n    &:hover {\n        box-shadow: $primary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $primary-color, $amount: 10%);\n    }\n\n    &ImgPlaceholderLetter {\n        color: $primary-color;\n    }\n}\n\n.secondaryBlue {\n    background-color: $secondary-color;\n\n    &:hover {\n        box-shadow: $secondary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $secondary-color, $amount: 10%);\n    }\n\n    &ImgPlaceholderLetter {\n        color: $secondary-color;\n    }\n}\n\n.tertiaryBlue {\n    background-color: $tertiary-color;\n\n    &:hover {\n        box-shadow: $tertiary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $tertiary-color, $amount: 10%);\n    }\n\n    &ImgPlaceholderLetter {\n        color: $tertiary-color;\n    }\n}\n\n.green {\n    background-color: $green-color;\n\n    &:hover {\n        box-shadow: $green-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $green-color, $amount: 10%);\n    }\n\n    &ImgPlaceholderLetter {\n        color: $green-color;\n    }\n}\n\n.orange {\n    background-color: $orange-color;\n\n    &:hover {\n        box-shadow: $orange-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $orange-color, $amount: 10%);\n    }\n\n    &ImgPlaceholderLetter {\n        color: $orange-color;\n    }\n}\n\n.red {\n    background-color: $red-color;\n\n    &:hover {\n        box-shadow: $red-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $red-color, $amount: 10%);\n    }\n\n    .MemberCardImgPlaceholderLetter {\n        color: $red-color;\n    }\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}"},"lineCount":null}},"error":null,"hash":"63d700dd007d0d2fb52c1c6c0e06b491","cacheData":{"env":{}}}