x-shadow: $primary-box-shadow;\n    transition: box-shadow 0.2s ease-in-out;\n\n    @include media-query($breakpoint: $tablet-breakpoint) {\n        width: 410px;\n        height: 200px;\n    }\n\n    &Name {\n        @include text(\n            $size: 1.3rem,\n            $weight: 400,\n            $color: $white-color,\n            $line-height: 1.8rem\n        );\n        text-align: center;\n        max-height: 35px;\n        margin: 20px 20px 0;\n        white-space: nowrap;\n        overflow: hidden;\n        text-overflow: ellipsis;\n    }\n\n    &Composers {\n        @include text(\n            $size: 1rem,\n            $weight: 300,\n            $color: $white-color,\n            $line-height: 1.25rem\n        );\n        text-align: center;\n        max-height: 22px;\n        margin: 20px 20px 0;\n        white-space: nowrap;\n        overflow: hidden;\n        text-overflow: ellipsis;\n    }\n\n    &DivisionLine {\n        display: block;\n        border-top: 1px solid $white-color;\n        margin: 20px 20px 0;\n    }\n\n    &Buttons {\n        @include flex-container(\n            $direction: column,\n            $justify-content: space-between,\n            $align-items: center,\n            $wrap: nowrap\n        );\n        margin: 20px 20px 0;\n        height: 96px;\n\n        @include media-query($breakpoint: $tablet-breakpoint) {\n            flex-direction: row;\n            height: auto;\n        }\n    }\n}\n\n.primaryBlue {\n    background-color: $primary-color;\n\n    &:hover {\n        box-shadow: $primary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $primary-color, $amount: 10%);\n    }\n}\n\n.secondaryBlue {\n    background-color: $secondary-color;\n\n    &:hover {\n        box-shadow: $secondary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $secondary-color, $amount: 10%);\n    }\n}\n\n.tertiaryBlue {\n    background-color: $tertiary-color;\n\n    &:hover {\n        box-shadow: $tertiary-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $tertiary-color, $amount: 10%);\n    }\n}\n\n.green {\n    background-color: $green-color;\n\n    &:hover {\n        box-shadow: $green-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $green-color, $amount: 10%);\n    }\n}\n\n.orange {\n    background-color: $orange-color;\n\n    &:hover {\n        box-shadow: $orange-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $orange-color, $amount: 10%);\n    }\n}\n\n.red {\n    background-color: $red-color;\n\n    &:hover {\n        box-shadow: $red-color-box-shadow;\n    }\n\n    &Darken {\n        background-color: darken($color: $red-color, $amount: 10%);\n    }\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507ca1;\n$tertiary-color: #7e9fa6;\n$white-color: #f8f8f8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47a65c;\n$yellow-color: #e2964d;\n$orange-color: #f09033;\n$red-color: #df5656;\n$background-color: #f8f8f8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px\n    rgba(\n        $color: #898989,\n        $alpha: 0.7,\n    );\n$primary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $primary-color,\n        $alpha: 1,\n    );\n$secondary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $secondary-color,\n        $alpha: 1,\n    );\n$tertiary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $tertiary-color,\n        $alpha: 1,\n    );\n$green-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $green-color,\n        $alpha: 1,\n    );\n$orange-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $orange-color,\n        $alpha: 1,\n    );\n$red-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $red-color,\n        $alpha: 1,\n    );\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container(\n    $grid-template-rows,\n    $grid-template-columns,\n    $align-items\n) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}\n"},"lineCount":null}},"error":null,"hash":"5a25b0dbddea91c7eb87907fedbb4f29","cacheData":{"env":{}}}