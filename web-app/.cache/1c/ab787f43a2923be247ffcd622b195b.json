ing: 20px 0 4px;\n    border: none;\n    outline: none;\n    background-color: $white-color;\n\n    &:focus + .label .content,\n    &Valid + .label .content {\n        transform: translateY(-150%);\n        @include text(\n            $size: 0.8rem,\n            $weight: 400,\n            $color: $primary-color,\n            $line-height: null\n        );\n    }\n\n    &:focus + .label::after,\n    &Valid + .label::after {\n        transform: translateX(0%);\n    }\n\n    &Container {\n        position: relative;\n        overflow: hidden;\n    }\n}\n\n.label {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    pointer-events: none;\n    border-bottom: 1px solid black;\n\n    &::after {\n        content: \"\";\n        position: absolute;\n        bottom: -1px;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        border-bottom: 3px solid $primary-color;\n        transform: translateX(-100%);\n        transition: transform 0.3s ease;\n    }\n}\n\n.content {\n    position: absolute;\n    bottom: 5px;\n    left: 0px;\n    @include text(\n        $size: 1rem,\n        $weight: 400,\n        $color: $dark-gray-color,\n        $line-height: null\n    );\n    transition: all 0.3s ease;\n}\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container(\n    $grid-template-rows,\n    $grid-template-columns,\n    $align-items\n) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507ca1;\n$tertiary-color: #7e9fa6;\n$white-color: #f8f8f8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47a65c;\n$yellow-color: #e2964d;\n$orange-color: #f09033;\n$red-color: #df5656;\n$background-color: #f8f8f8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px\n    rgba(\n        $color: #898989,\n        $alpha: 0.7,\n    );\n$primary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $primary-color,\n        $alpha: 1,\n    );\n$secondary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $secondary-color,\n        $alpha: 1,\n    );\n$tertiary-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $tertiary-color,\n        $alpha: 1,\n    );\n$green-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $green-color,\n        $alpha: 1,\n    );\n$orange-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $orange-color,\n        $alpha: 1,\n    );\n$red-color-box-shadow: 10px 10px 20px\n    rgba(\n        $color: $red-color,\n        $alpha: 1,\n    );\n"},"lineCount":null}},"error":null,"hash":"c3c85d9be33bb65c5e81d077d94b6937","cacheData":{"env":{}}}