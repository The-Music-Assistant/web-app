:703}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":11,"column":4},"generated":{"line":1,"column":704}},{"source":"../../src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss","name":null,"original":{"line":63,"column":29},"generated":{"line":1,"column":718}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":12,"column":4},"generated":{"line":1,"column":719}},{"source":"../../src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss","name":null,"original":{"line":63,"column":43},"generated":{"line":1,"column":734}},{"source":"../../src/root-styles/_mixins.scss","name":null,"original":{"line":13,"column":4},"generated":{"line":1,"column":735}},{"source":"../../src/root-styles/_variables.scss","name":null,"original":{"line":15,"column":25},"generated":{"line":1,"column":748}},{"source":"../../src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss","name":null,"original":{"line":64,"column":4},"generated":{"line":1,"column":749}},{"source":"../../src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss","name":null,"original":{"line":64,"column":29},"generated":{"line":1,"column":772}}],"sources":{"../../src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss":"// ----------------------------------------------------------------------------\n// File Path: src/components/FormInputs/TextInputs/LargeTextInput/LargeTextInput.module.scss\n// Description: SCSS styles for the LargeTextInput component\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 1/5/2020\n// ----------------------------------------------------------------------------\n\n@import \"../../../../root-styles/utilities.scss\";\n\n.input {\n    width: 100%;\n    height: 100%;\n    @include text($size: 1rem, $weight: 400, $color: $dark-gray-color, $line-height: null);\n    padding: 20px 0 4px;\n    border: none;\n    outline: none;\n    background-color: $white-color;\n\n    &:focus + .label .content,\n    &Valid + .label .content {\n        transform: translateY(-150%);\n        @include text($size: 0.8rem, $weight: 400, $color: $primary-color, $line-height: null);\n    }\n\n    &:focus + .label::after,\n    &Valid + .label::after {\n        transform: translateX(0%);\n    }\n\n    &Container {\n        position: relative;\n        overflow: hidden;\n    }\n}\n\n.label {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    pointer-events: none;\n    border-bottom: 1px solid black;\n\n    &::after {\n        content: \"\";\n        position: absolute;\n        bottom: -1px;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        border-bottom: 3px solid $primary-color;\n        transform: translateX(-100%);\n        transition: transform 0.3s ease;\n    }\n}\n\n.content {\n    position: absolute;\n    bottom: 5px;\n    left: 0px;\n    @include text($size: 1rem, $weight: 400, $color: $dark-gray-color, $line-height: null);\n    transition: all 0.3s ease;\n}\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n"},"lineCount":null}},"error":null,"hash":"02e45d0c04018d25855dc681cb195d0c","cacheData":{"env":{}}}