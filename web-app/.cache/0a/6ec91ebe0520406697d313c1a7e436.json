},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"resolve","original":{"line":48,"column":12},"generated":{"line":1,"column":1086}},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"destroy","original":{"line":53,"column":15},"generated":{"line":1,"column":1095}},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"destroy","original":{"line":53,"column":15},"generated":{"line":1,"column":1097}},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"destroy","original":{"line":53,"column":15},"generated":{"line":1,"column":1099}},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"destroy","original":{"line":53,"column":15},"generated":{"line":1,"column":1107}},{"source":"../../src/vendors/AlphaTab/destruction.js","name":"destroy","original":{"line":53,"column":15},"generated":{"line":1,"column":1115}}],"sources":{"../../src/vendors/AlphaTab/destruction.js":"/**\n * Destroys the AlphaTab api.\n * Ends pitch detection.\n * Removes the P5 drawing object.\n * @module alphaTabDestruction\n * @category AlphaTab\n * @author Daniel Griessler <dgriessler20@gmail.com>\n * @author Dan Levy <danlevy124@gmail.com>\n */\n\n// File imports\nimport atVars from \"./variables\";\nimport * as playerStates from \"./playerStates\";\nimport destroyPitchDetection from \"../ML5/PitchDetection/destruction\";\n\n/**\n * Destroys the api if initialized.\n * Destroys the p5Obj if initialized.\n * Stops the microphone input it is being used.\n * @function\n * @returns {Promise} Promise that resolves after api is destroyed and cleared\n */\nconst destroy = () => {\n    if (atVars.playerState === playerStates.PLAYING) {\n        // Stops the player\n        atVars.playerState = playerStates.PENDING_STOP;\n        atVars.api.stop();\n    }\n\n    // Ends pitch detection\n    destroyPitchDetection();\n\n    if (atVars.p5Obj !== null) {\n        // Removes the p5 canvas\n        atVars.p5Obj.remove();\n    }\n\n    // Returns a promise that waits for AlphaTab to stop playing\n    return new Promise((resolve) => {\n        // Waits for the player state to change to 0 (stopped) before destroying the api\n        atVars.api.addPlayerStateChanged(() => {\n            // Destroys the AlphaTab api\n            atVars.api.destroy();\n\n            // Reinitializes AlphaTab variables\n            atVars.initialize();\n\n            resolve();\n        });\n    });\n};\n\nexport default destroy;\n"},"lineCount":null}},"error":null,"hash":"61c262ade783d694c142de38b4dc167a","cacheData":{"env":{}}}