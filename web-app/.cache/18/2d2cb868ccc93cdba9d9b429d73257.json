n":786}},{"source":"../../src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss","name":null,"original":{"line":56,"column":8},"generated":{"line":1,"column":787}},{"source":"../../src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss","name":null,"original":{"line":56,"column":24},"generated":{"line":1,"column":802}},{"source":"../../src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss","name":null,"original":{"line":59,"column":8},"generated":{"line":1,"column":803}},{"source":"../../src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss","name":null,"original":{"line":61,"column":24},"generated":{"line":1,"column":823}}],"sources":{"../../src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss":"// ----------------------------------------------------------------------------\n// File Path: src/components/ChoirSelection/ChoirCards/ChoirCard/ChoirCard.module.scss\n// Description: SCSS styles for the choir card component\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 2/19/2020\n// ----------------------------------------------------------------------------\n\n@import \"../../../../root-styles/utilities.scss\";\n\n.choirCard {\n    width: 280px;\n    height: 280px;\n    border-radius: 10px;\n    box-shadow: $primary-box-shadow;\n    transition: box-shadow 0.2s ease-in-out;\n    cursor: pointer;\n\n    &HeaderImg {\n        object-fit: cover;\n        width: 100%;\n        height: 70px;\n\n        &Placeholder {\n            @include flex-container(\n                $direction: row,\n                $justify-content: center,\n                $align-items: center,\n                $wrap: nowrap\n            );\n            width: 100%;\n            height: 70px;\n            background-color: darken($color: $primary-color, $amount: 10%);\n            border-radius: 10px 10px 0 0;\n\n            &Img {\n                height: 30px;\n            }\n        }\n    }\n\n    &Name {\n        @include text($size: 1.3rem, $weight: 400, $color: $white-color, $line-height: 1.8rem);\n        text-align: center;\n        max-height: 55px;\n        margin: 20px 20px 0;\n        display: -webkit-box;\n        -webkit-line-clamp: 2;\n        -webkit-box-orient: vertical;\n        overflow: hidden;\n    }\n\n    &Description {\n        @include text($size: 1rem, $weight: 300, $color: $white-color, $line-height: 1.25rem);\n        text-align: center;\n        max-height: 75px;\n        margin: 20px 20px 0;\n        display: -webkit-box;\n        -webkit-line-clamp: 3;\n        -webkit-box-orient: vertical;\n        overflow: hidden;\n    }\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}"},"lineCount":null}},"error":null,"hash":"4302db3db7d38b62260ba2a191862b4b","cacheData":{"env":{}}}