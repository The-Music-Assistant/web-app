rtBar/AlertBar.module.scss","name":null,"original":{"line":67,"column":4},"generated":{"line":1,"column":822}},{"source":"../../src/components/AlertBar/AlertBar.module.scss","name":null,"original":{"line":68,"column":8},"generated":{"line":1,"column":836}},{"source":"../../src/root-styles/_variables.scss","name":null,"original":{"line":10,"column":25},"generated":{"line":1,"column":873}}],"sources":{"../../src/components/AlertBar/AlertBar.module.scss":"// ----------------------------------------------------------------------------\n// File Path: src/components/AuthBar/AuthBar.module.scss\n// Description: SCSS styles for the auth bar component\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 1/20/2020\n// ----------------------------------------------------------------------------\n\n@import \"../../root-styles/utilities.scss\";\n\n.alertBar {\n    position: fixed;\n    top: 10px;\n    left: 50%;\n    transform: translate(-50%, -140%);\n    width: calc(100vw - 20px);\n    max-width: 400px;\n    padding: 10px 20px;\n    box-shadow: $primary-box-shadow;\n    z-index: 10000;\n    border-radius: 10px;\n    transition: transform 1s ease-out;\n\n    &SlideDown {\n        transform: translate(-50%, 0);\n    }\n\n    &SlideUp {\n        transform: translate(-50%, -140%);\n    }\n\n    &TopGrid {\n        @include grid-container(\n            $grid-template-rows: 1fr,\n            $grid-template-columns: 15px 1fr 15px,\n            $align-items: center\n        );\n    }\n\n    &Heading {\n        @include text($size: 1.25rem, $weight: 400, $color: $white-color, $line-height: null);\n        text-align: center;\n    }\n\n    &Message {\n        @include text($size: 1rem, $weight: 300, $color: $white-color, $line-height: null);\n        text-align: center;\n        margin-top: 5px;\n    }\n\n    &CloseButtonImg {\n        width: 15px;\n    }\n\n    &Success {\n        background-color: rgba($green-color, 0.95);\n    }\n\n    &Warning {\n        background-color: rgba($yellow-color, 0.95);\n    }\n\n    &Error {\n        background-color: rgba($red-color, 0.95);\n    }\n\n    &Info {\n        background-color: rgba($secondary-color, 0.95);\n    }\n}\n","../../src/root-styles/_variables.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_variables.scss\n// Description: SCSS variables used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n$primary-color: #5f9cd1;\n$secondary-color: #507CA1;\n$tertiary-color: #7E9FA6;\n$white-color: #F8F8F8;\n$light-gray-color: #d4d4d4;\n$gray-color: #4e4e4e;\n$dark-gray-color: #212121;\n$green-color: #47A65C;\n$yellow-color: #E2964D;\n$orange-color: #F09033;\n$red-color: #DF5656;\n$background-color: #F8F8F8;\n$modal-background-color: rgba($primary-color, 0.5);\n\n$tablet-breakpoint: 768px;\n$desktop-breakpoint: 1024px;\n$large-desktop-breakpoint: 1400px;\n\n$primary-box-shadow: 10px 10px 20px rgba($color: #898989, $alpha: 0.7);\n$primary-color-box-shadow: 10px 10px 20px rgba($color: $primary-color, $alpha: 1);\n$secondary-color-box-shadow: 10px 10px 20px rgba($color: $secondary-color, $alpha: 1);\n$tertiary-color-box-shadow: 10px 10px 20px rgba($color: $tertiary-color, $alpha: 1);\n$green-color-box-shadow: 10px 10px 20px rgba($color: $green-color, $alpha: 1);\n$orange-color-box-shadow: 10px 10px 20px rgba($color: $orange-color, $alpha: 1);\n$red-color-box-shadow: 10px 10px 20px rgba($color: $red-color, $alpha: 1);\n","../../src/root-styles/_mixins.scss":"// ----------------------------------------------------------------------------\n// File Path: src/root-styles/_mixins.scss\n// Description: SCSS mixins used throughout all scss files\n// Author: Dan Levy\n// Email: danlevy124@gmail.com\n// Created Date: 10/23/2019\n// ----------------------------------------------------------------------------\n\n// Formats text\n@mixin text($size, $weight, $color, $line-height) {\n    font-size: $size;\n    font-weight: $weight;\n    color: $color;\n    line-height: $line-height;\n}\n\n// Media query\n@mixin media-query($breakpoint) {\n    @media screen and (min-width: $breakpoint) {\n        @content;\n    }\n}\n\n// Flexbox settings\n@mixin flex-container($direction, $justify-content, $align-items, $wrap) {\n    display: flex;\n    flex-direction: $direction;\n    justify-content: $justify-content;\n    align-items: $align-items;\n    flex-wrap: $wrap;\n}\n\n@mixin grid-container($grid-template-rows, $grid-template-columns, $align-items) {\n    display: grid;\n    grid-template-rows: $grid-template-rows;\n    grid-template-columns: $grid-template-columns;\n    align-items: $align-items;\n}"},"lineCount":null}},"error":null,"hash":"b3b9bb50dfbd35c65fac5a8d6d0e8e31","cacheData":{"env":{}}}